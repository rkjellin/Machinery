<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="text/x-cargo-project">
    <sub-class-of type="text/plain"/>
    <comment>Cargo project file</comment>
    <glob pattern="*.toml"/>
  </mime-type>

  <mime-type type="text/x-rust">
    <sub-class-of type="text/plain"/>
    <comment>Rust source file</comment>
    <glob pattern="*.rs"/>
  </mime-type>
</mime-info>
